@article{LeSage_Pace_2014,
	title        = {The Biggest Myth in Spatial Econometrics},
	volume       = {2},
	issn         = {2225-1146},
	doi          = {10.3390/econometrics2040217},
	abstractnote = {There is near universal agreement that estimates and inferences from spatial regression models are sensitive to particular speciﬁcations used for the spatial weight structure in these models. We ﬁnd little theoretical basis for this commonly held belief, if estimates and inferences are based on the true partial derivatives for a well-speciﬁed spatial regression model. We conclude that this myth may have arisen from past applied work that incorrectly interpreted the model coefﬁcients as if they were partial derivatives, or from use of misspeciﬁed models.},
